// This is your Prisma schema file
// Docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  usn       String   @unique
  dob       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
